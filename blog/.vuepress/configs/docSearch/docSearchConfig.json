{
  "index_name": "blog",
  "start_urls": [
    "https://blog.qbb.sh/tags/",
    "https://blog.qbb.sh/docs/play/",
    "https://blog.qbb.sh/docs/fontend/"
  ],
  "stop_urls": [
    "!https://blog.qbb.sh"
  ],
  "selectors": {
    "lvl0": {
      "selector": ".sidebar-heading.active",
      "global": true,
      "default_value": "Documentation"
    },
    "lvl1": ".theme-container h1",
    "lvl2": ".theme-container h2",
    "lvl3": ".theme-container h3",
    "lvl4": ".theme-container dt",
    "lang": {
      "selector": "/html/@lang",
      "type": "xpath",
      "global": true
    }
  },
  "custom_settings": {
    "attributesForFaceting": ["lang"]
  }
}